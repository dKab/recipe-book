{
  "name": "recipy-book",
  "version": "1.0.0",
  "description": "recipy-book allows you to store your favorite recipies",
  "main": "index.js",
  "babel": {
    "env": {
      "server": {
        "presets": [
          "es2017",
          "react"
        ]
      }
    }
  },
  "scripts": {
    "predev": "rimraf public/bundle.js",
    "dev": "cross-env NODE_ENV=development nodemon --watch src/server --harmony-async-await src/server/server.js",
    "prestart": "webpack --config config/webpack.config.prod.js && cross-env BABEL_ENV=server babel src/server/server.js --out-file src/server/server-compiled.js",
    "start": "cross-env NODE_ENV=prod node src/server/server-compiled.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dKab/recipy-book.git"
  },
  "keywords": [
    "recipy",
    "recipies",
    "recipy-book"
  ],
  "author": "dKab",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dKab/recipy-book/issues"
  },
  "homepage": "https://github.com/dKab/recipy-book#readme",
  "dependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.22.2",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2017": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "cross-env": "^3.1.4",
    "koa": "next",
    "koa-router": "next",
    "koa-static": "^2.1.0",
    "koa-webpack": "^0.2.2",
    "nodemon": "^1.11.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "next",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "rimraf": "^2.5.4",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.17.0",
    "webpack-merge": "^2.6.1"
  }
}
